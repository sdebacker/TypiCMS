{
  "name": "TypiCMS",
  "dependencies": {
    "jquery": "~1.*",
    "alertify.js": "~0.3.*",
    "bootstrap": "~3.2.*",
    "dropzone": "~3.*",
    "jquery-ui": "~1.10.*",
    "fancybox": "~2.1.*",
    "font-awesome": "~4.*",
    "selectize": "~0.*",
    "pickadate": "~3.5.2",
    "fastclick": "~1.0.2",
    "jsurl": "*",
    "angular": "~1.2.25",
    "angular-resource": "~1.2",
    "angular-smart-table": "~1.3.0",
    "angular-i18n": "~1.2.25",
    "angular-gettext": "~1.1.2",
    "swiper": "~2.7.0"
  },
  "overrides": {
    "fancybox": {
      "main": [
        "source/jquery.fancybox.js"
      ]
    },
    "bootstrap": {
      "main": [
        "js/dropdown.js",
        "js/collapse.js",
        "js/alert.js",
        "js/tab.js",
        "js/transition.js"
      ]
    },
    "jquery-ui": {
      "main": [
        "ui/jquery-ui.js",
        "ui/jquery.ui.core.js",
        "ui/jquery.ui.mouse.js",
        "ui/jquery.ui.widget.js",
        "ui/jquery.ui.sortable.js"
      ]
    },
    "pickadate": {
      "main": [
        "lib/picker.js",
        "lib/picker.date.js",
        "lib/picker.time.js"
      ]
    },
    "font-awesome": {
      "ignore": true
    },
    "angular-i18n": {
      "ignore": true
    },
    "jsurl": {
      "main": [
        "url.js"
      ]
    }
  }
}
