{
  "name": "TypiCMS",
  "dependencies": {
    "jquery": "~2.0.*",
    "jquery-legacy": "jquery#1.10",
    "alertify.js": "~0.3.*",
    "bootstrap": "~3.1.*",
    "dropzone": "~3.7.*",
    "jquery-ui": "~1.10.*",
    "fancybox": "~2.1.*",
    "flexslider": "*",
    "select2": "~3.4.*",
    "font-awesome": "~4.0.3",
    "eonasdan-bootstrap-datetimepicker": "~3.*"
  },
  "overrides": {
    "alertify.js": {
      "main": [
        "themes/alertify.bootstrap.css",
